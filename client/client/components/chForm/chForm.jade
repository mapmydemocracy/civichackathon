div
  h1.h1.Bootstrap {{title}}

  div(ng-repeat="item in items")

    div.row
      div.col-md-8
        h3(ng-click="stepForward()") {{item.name}}

  div.row
    form.col-md-8
      div.form-group
        input#inputjurisdictionname.form-control(ng-model="newInput",type="text",placeholder="{{type}} name")
      div.form-group
        button(type="button",ng-click="add[type](newInput)").btn.btn-primary
          text Add

